{
    "build": {
        "dockerfile": "docker/node/Dockerfile",
        "args": {
            "ID_GROUP": "1000",
            "ID_USER": "1000",
            "VERSION_NODE": "20.9.0"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "Vue.volar",
                "Vue.vscode-typescript-vue-plugin",
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "streetsidesoftware.code-spell-checker",
                "ms-vscode.makefile-tools"
            ]
        }
    }
}
